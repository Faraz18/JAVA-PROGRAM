import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
		System.out.println("Enter number:");
		int n= sc.nextInt();
		int ori,a,c=0;
		
		 ori= n;
   
    while(ori!=0)
    {
       a=ori%10;
       c= c+a*a*a;
       ori/=10;
    }
    if (c==n)
    {
      System.out.println("ARMSTRONG");
    }
    else
    {
       System.out.println("NOT ARMSTRONG");
    }
	}
}
